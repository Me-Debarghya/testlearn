{
    "version": 2,
    "builds": [
        {
            "ignoreCommand": "git diff --quiet HEAD^ HEAD -- ./database/"
        },
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}